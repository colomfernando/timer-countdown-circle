{
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"ecmaFeatures": {
		"jsx": true
	},
	"plugins": ["@typescript-eslint", "react-hooks"],
	"extends": [
		"plugin:react/recommended",
		"plugin:@typescript-eslint/recommended",
		"plugin:prettier/recommended"
	],
	"rules": {
		"react/prop-types": "off"
	},
	"settings": {
		"react": {
			"pragma": "React",
			"version": "detect"
		}
	}
}
